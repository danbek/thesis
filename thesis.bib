@article{bock_novel_1995,
	title = {A novel bolometer for infrared and millimeter-wave astrophysics},
	volume = {74},
	issn = {0038-6308, 1572-9672},
	url = {http://link.springer.com/article/10.1007/BF00751274},
	doi = {10.1007/BF00751274},
	abstract = {We are developing a novel bolometer which uses a fine mesh to absorb radiation. The filling factor of the mesh is small, providing a small heat capacity and a low geometric cross-section to cosmic rays. The mesh is patterned from a free-standing silicon nitride membrane and is thermally isolated by long radial legs of silicon nitride. A thin metallic film evaporated on the mesh absorbs radiation by matching the surface impedance to that of free space. A neutron transmutation doped germanium thermistor attached to the center of the mesh detects the temperature increase from absorbed radiation. The low thermal conductivity and heat capacity of silicon nitride provide improved performance in low background applications. We discuss the theoretical limits of the performance of these devices. We have tested a device at 300 {mK} with an {electricalNEP=4×10−17} W Hz−1/2 and a time constant τ=40 ms.},
	pages = {229-235},
	number = {1},
	journaltitle = {Space Science Reviews},
	shortjournal = {Space Sci Rev},
	author = {Bock, J. J. and Chen, D. and Mauskopf, P. D. and Lange, A. E.},
	urldate = {2014-03-06},
	date = {1995-10-01},
	langid = {english},
	keywords = {Astronomy, Astrophysics and Cosmology, Extraterrestrial Physics, Space Sciences},
	file = {Snapshot:C:\Users\beckerd\AppData\Roaming\Zotero\Zotero\Profiles\8q21ck9m.default\zotero\storage\PPV7F36R\10.html:text/html}
}

@article{clarke_superconductive_1977,
	title = {Superconductive bolometers for submillimeter wavelengths},
	volume = {48},
	issn = {0021-8979, 1089-7550},
	url = {http://scitation.aip.org/content/aip/journal/jap/48/12/10.1063/1.323612},
	doi = {10.1063/1.323612},
	abstract = {Three types of composite superconductive bolometers are described in which the temperature‐sensitive element is a superconducting film at the transition temperature, a superconductor-normal metal-superconductor Josephson junction, or a superconductor-insulator-normal metal quasiparticle tunneling junction. The temperature-sensitive element is evaporated onto a sapphire substrate on the reverse side of which is a bismuth film to absorb the submillimeter radiation. The noise limitations of each type of bolometer are calculated. The fabrication and measured performance of the transition-edge bolometer and the Josephson-junction bolometer are described. The best electrical (noise‐equivalent power) {NEP} obtained with a transition-edge bolometer fabricated on a 4x4x0.005 mm sapphire substrate is (1.7 +/- 0.1) {\textbackslash}times 10{\textasciicircum}-15 W / rt-Hz at 2 Hz at an operating temperature of 1.27 K. This {NEP} is within a factor of 2 of the thermal noise limit. The effective absorptivity of the bismuth film is measured to be 0.47 +/- 0.05, and the corresponding detectivity D* is calculated to be (1.1 +/- 0.1) 10{\textasciicircum}14 cm W−1 Hz{\textasciicircum}1/2. Suggestions are made for further improvements in sensitivity.},
	pages = {4865-4879},
	number = {12},
	journaltitle = {Journal of Applied Physics},
	author = {Clarke, J. and Hoffer, G. I. and Richards, P. L. and Yeh, N.-H.},
	urldate = {2014-03-06},
	date = {1977-12},
	keywords = {Bismuth, Bolometers, Collective excitations, Josephson junctions, Sapphire, {SUPERCONDUCTING} {FILMS}, Superconducting metals, Superconducting transitions, Transition metals, Tunneling},
	file = {Full Text PDF:C:\Users\beckerd\AppData\Roaming\Zotero\Zotero\Profiles\8q21ck9m.default\zotero\storage\M5AWGQF4\Clarke et al. - 2008 - Superconductive bolometers for submillimeter wavel.pdf:application/pdf;Snapshot:C:\Users\beckerd\AppData\Roaming\Zotero\Zotero\Profiles\8q21ck9m.default\zotero\storage\EW3HX6KQ\1.html:text/html}
}

@article{pardo_atmospheric_2001,
