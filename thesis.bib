@article{petkie_active_2008,
	title = {Active and passive imaging in the {THz} spectral region: phenomenology, dynamic range, modes, and illumination},
	volume = {25},
	url = {http://josab.osa.org/abstract.cfm?URI=josab-25-9-1523},
	doi = {10.1364/JOSAB.25.001523},
	shorttitle = {Active and passive imaging in the {THz} spectral region},
	abstract = {The useful compromise between resolution and penetration power of the submillimeter or terahertz ({THz)} spectral region has long made it attractive for a variety of imaging applications. However, many of the demonstrations of imaging in this spectral region have used strategically oriented targets, especially favorable concealment materials, proximate imaging geometries, etc. This paper reports the results of studies aimed at better understanding the phenomenology of targets, the impact of this phenomenology on various active and passive imaging strategies, and most importantly, the development of imaging strategies that do not require the aforementioned special circumstances. Particular attention is paid to the relationship between active and passive images, especially with respect to how they interact with the illumination- and detector-mode structures of various imaging scenarios. It is concluded that the very large dynamic range that can be obtained with active single-mode systems (including focal-plane arrays) can be used in system designs to overcome the deleterious effects that result from the dominance of specular reflections in single-mode active systems as well as to strategically orient targets to obtain recognition. This will aid in the development of a much more robust and generally useful imaging technology in this spectral region.},
	pages = {1523-1531},
	number = {9},
	journaltitle = {Journal of the Optical Society of America B},
	shortjournal = {J. Opt. Soc. Am. B},
	author = {Petkie, Douglas T. and Casto, Corey and De Lucia, Frank C. and Murrill, Steven R. and Redman, Brian and Espinola, Richard L. and Franck, Charmaine C. and Jacobs, Eddie L. and Griffin, Steven T. and Halford, Carl E. and Reynolds, Joe and {O'Brien}, Sean and Tofsted, David},
	urldate = {2014-03-06},
	date = {2008-09-01},
	keywords = {Radiometry},
	file = {J. Opt. Soc. Am. B Full Text PDF:C:\Users\beckerd\AppData\Roaming\Zotero\Zotero\Profiles\8q21ck9m.default\zotero\storage\XV4ESD9F\Petkie et al. - 2008 - Active and passive imaging in the THz spectral reg.pdf:application/pdf;J. Opt. Soc. Am. B Snapshot:C:\Users\beckerd\AppData\Roaming\Zotero\Zotero\Profiles\8q21ck9m.default\zotero\storage\WU7KWRCI\abstract.html:text/html}
}

@article{goodman_fundamental_1976,
	title = {Some fundamental properties of speckle},
	volume = {66},
	url = {http://www.opticsinfobase.org/abstract.cfm?URI=josa-66-11-1145},
	doi = {10.1364/JOSA.66.001145},
	abstract = {A speckle pattern formed in polarized monochromatic light may be regarded as resulting from a classical random walk in the complex plane. The resulting irradiance fluctuations obey negative exponential statistics, with ratio of standard deviation to mean (i.e., contrast) of unity. Reduction of this contrast, or smoothing of the speckle, requires diversity in polarization, space, frequency, or time. Addition of M uncorrelated speckle patterns on an intensity basis can reduce the contrast by 1/√ M. However, addition of speckle patterns on a complex amplitude basis provides no reduction of contrast. The distribution of scale sizes in a speckle pattern (i.e., the Wiener spectrum) is investigated from a physical point of view.},
	pages = {1145-1150},
	number = {11},
	journaltitle = {Journal of the Optical Society of America},
	shortjournal = {J. Opt. Soc. Am.},
	author = {Goodman, J. W.},
	urldate = {2014-03-06},
	date = {1976-11-01},
	file = {J. Opt. Soc. Am. Full Text PDF:C:\Users\beckerd\AppData\Roaming\Zotero\Zotero\Profiles\8q21ck9m.default\zotero\storage\B5PE6AWJ\Goodman - 1976 - Some fundamental properties of speckle.pdf:application/pdf;J. Opt. Soc. Am. Snapshot:C:\Users\beckerd\AppData\Roaming\Zotero\Zotero\Profiles\8q21ck9m.default\zotero\storage\7NKACHE7\abstract.html:text/html}
}

