	keywords = {Astrophysics - Cosmology and Extragalactic Astrophysics},
	file = {1110.2101 PDF:C:\Users\beckerd\AppData\Roaming\Zotero\Zotero\Profiles\8q21ck9m.default\zotero\storage\R9K3BQGQ\Keating et al. - 2011 - Ultra High Energy Cosmology with POLARBEAR.pdf:application/pdf;arXiv.org Snapshot:C:\Users\beckerd\AppData\Roaming\Zotero\Zotero\Profiles\8q21ck9m.default\zotero\storage\JHP66E5G\1110.html:text/html}
