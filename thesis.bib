	keywords = {Metrological instrumentation, Metrology, Optical standards and testing, Radiometry},
	file = {Appl. Opt. Full Text PDF:C:\Users\beckerd\AppData\Roaming\Zotero\Zotero\Profiles\8q21ck9m.default\zotero\storage\PB9CKBBX\Dietlein et al. - 2008 - Aqueous blackbody calibration source for millimete.pdf:application/pdf;Appl. Opt. Snapshot:C:\Users\beckerd\AppData\Roaming\Zotero\Zotero\Profiles\8q21ck9m.default\zotero\storage\SUJD42C8\abstract.html:text/html}
