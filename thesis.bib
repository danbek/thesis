@article{rogalski_semiconductor_2013,
	title = {Semiconductor detectors and focal plane arrays for far-infrared imaging},
	volume = {21},
	issn = {1230-3402, 1896-3757},
	url = {http://link.springer.com/article/10.2478/s11772-013-0110-x},
