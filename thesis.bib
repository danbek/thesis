@article{ullom_characterization_2004,
	title = {Characterization and reduction of unexplained noise in superconducting transition-edge sensors},
	volume = {84},
	issn = {0003-6951, 1077-3118},
	url = {http://scitation.aip.org/content/aip/journal/apl/84/21/10.1063/1.1753058},
	doi = {10.1063/1.1753058},
	abstract = {The noise in superconducting transition-edge sensors ({TESs)} commonly exceeds simple theoretical predictions. The reason for this discrepancy is presently unexplained. We have measured the amplitude and frequency dependence of the noise in {TES} sensors with eight different geometries. In addition, we have measured the dependence of the noise on operating resistance, perpendicular magnetic field, and bath temperature. We find that the unexplained noise contribution is inversely correlated with the temperature width of the superconducting-to-normal transition and is reduced by a perpendicular field and in certain geometries. These results suggest paths to improved sensor performance.},
	pages = {4206-4208},
	number = {21},
	journaltitle = {Applied Physics Letters},
	author = {Ullom, J. N. and Doriese, W. B. and Hilton, G. C. and Beall, J. A. and Deiker, S. and Duncan, W. D. and Ferreira, L. and Irwin, K. D. and Reintsema, C. D. and Vale, L. R.},
	urldate = {2014-03-01},
	date = {2004-05-07},
	keywords = {Curie point, Magnetic field measurements, Magnetic field sensors, Magnetic fields, Superconducting detectors, Superconducting transitions, Superconductivity, Superconductivity models},
	file = {Full Text PDF:C:\Users\beckerd\AppData\Roaming\Zotero\Zotero\Profiles\8q21ck9m.default\zotero\storage\GS5557C4\Ullom et al. - 2004 - Characterization and reduction of unexplained nois.pdf:application/pdf;Snapshot:C:\Users\beckerd\AppData\Roaming\Zotero\Zotero\Profiles\8q21ck9m.default\zotero\storage\UCWG4NN2\1.html:text/html}
}

@article{persky_review_1999,
