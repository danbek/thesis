	file = {Full Text PDF:C:\Users\beckerd\AppData\Roaming\Zotero\Zotero\Profiles\8q21ck9m.default\zotero\storage\7NBT9TA7\Rogalski and Sizov - 2011 - Terahertz detectors and focal plane arrays.pdf:application/pdf;Snapshot:C:\Users\beckerd\AppData\Roaming\Zotero\Zotero\Profiles\8q21ck9m.default\zotero\storage\KDEVIBRU\10.html:text/html}
